@model gameonfinal.Models.Users

@{ ViewData["Title"] = "Show Profile";
                Layout = "~/Views/Shared/_Layout.cshtml"; }

<h2>Showing Profile for this dater.</h2>

<div>
    <h4>user Profile</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.FirstName)
        </dt>
        <dd>
            @Html.DisplayNameFor(model => model.LastName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd>
            @Html.DisplayNameFor(model => model.Dateaccountcreated)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Displayname)
        </dt>
        <dd>
            @Html.DisplayNameFor(model => model.UserAccountId)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.PhotoPath)
        </dt>
        
    </dl>
</div>
<div>
    <a asp-action="Browse">Back to Browse</a>
    <a asp-controller="Messages" asp-action="NewMessage" asp-route-id="@Model.Id">Send Message </a>
</div>
